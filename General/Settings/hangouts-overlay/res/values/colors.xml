<?xml version="1.0" encoding="utf-8"?>
<!--You are editing Settings' Colors-->

<resources>			
	<!-- Edits the background in settings-->
		<color name="dashboard_background_color">#ff000000</color>
     	<color name="dashboard_category_background_color">#ff000000</color>

	<!--Toggle background color-->
		<color name="switchbar_background_color">#ff373737</color>
		<color name="switch_accent_color">@android:color/white</color>
	
	<!--Action Bar Colors-->
		<color name="theme_primary">#212021</color>
		<color name="theme_primary_dark">#ff212021</color>
	
	<!--Title text color and toggle colors-->
		<color name="theme_accent">@android:color/white</color>
	
    <!--QS Tile Cards inside Select and Order Tiles and Icon Color-->
		<color name="cardview_light_background">@android:color/transparent</color>
		<color name="qs_tile_tint_color">#ffffffff</color>
	
	<!--Remainder coloring for Data Usage and Battery Usage-->
		<color name="material_empty_color_light">#80777777</color>
	
	<!--Running Apps Percentage Bar-->
		<color name="running_processes_system_ram">#ffaba3ab</color>
		<color name="running_processes_apps_ram">@android:color/white</color>
		<color name="running_processes_free_ram">@android:color/transparent</color>
	
	<!--Pattern Lock Color-->
		<color name="lock_pattern_view_regular_color">#ff777777</color>
	
	<!--Increasing Ring Icon Disabled Color-->
		<color name="audio_ringer_disabled_tint">#4a777777</color>

	<color name="card_background">#ff000000</color>
</resources>
